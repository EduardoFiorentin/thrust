                    "generator_type": "random_int",
                    "min": 0,
                    "max": 100
                },
                "telefone": {
                    "generator_type": "phone"
                }
            }
        },
        
        "items": {
            "num_records": 20,
            "columns": {
                "id": {
                    "generator_type": "random_int",
                    "min": 0,
                    "max": 100
                },
                "nome": {
                    "generator_type": "random_int",
                    "min": 0,
                    "max": 100
                },
                "telefone": {
                    "generator_type": "phone",
                    "country": "pt-br"
                }
            }
        },
        "permissoes": {
            "num_records": 30,
            "columns": {
                "id": {
                    "generator_type": "random_int",
                    "min": 0,
                    "max": 100
                },
                "nome": {
                    "generator_type": "random_int",
                    "min": 0,
                    "max": 100
                }
            }
        },
        "testes_tipos": {
            "num_records": 150,
            "columns": {
                "serial": {
                    "generator_type": "serial",
