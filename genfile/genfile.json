                "random_int": {
                    "generator_type": "random_int",
                    "min": 0,
                    "max": 100
                },
                "phone": {
                    "generator_type": "phone",
                    "country": "pt-br"
                },
                "name_random_string": {
                    "generator_type": "random_string",
                    "min_len": 0,
                    "max_len": 20
                },
                "name" : {
                    "generator_type": "name_generator",
                    "min_len": 0,
                    "max_len": 20
                },
                "cpf": {
                    "generator_type": "cpf_generator"
                },
                "email": {
                    "generator_type": "email_generator",
                    "min_len": 0,
                    "max_len": 20
                }
            }
        }
    }
